{
  "name": "@pikadun/eslint-config",
  "version": "1.0.0",
  "description": "A shareable eslint config.",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky",
    "commitlint": "commitlint --config ./eng/.commitlintrc.json --edit",
    "eslint": "eslint --config ./eng/eslint.config.js --fix .",
    "cspell": "cspell --config ./eng/cspell.config.json .",
    "lint-staged": "lint-staged --config ./eng/.lintstagedrc.json"
  },
  "dependencies": {
    "@eslint/js": "^9.0.0",
    "@stylistic/eslint-plugin": "^1.7.2",
    "globals": "^15.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "cspell": "^8.7.0",
    "eslint": "^9.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  }
}
